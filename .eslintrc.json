{
  "env": {
    "commonjs": false,
    "es6": true,
    "node": true
  },
  "extends": ["google", "prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "prettier/prettier": "error"
  },
  "plugins": ["prettier"]
}
