{
  "name": "justclean-towers",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prestart": "npm run test:eslint",
    "start": "node index.js",
    "test:prettier": "prettier --list-different \"**/*.{js,css,scss,md}\"",
    "fix:prettier": "prettier --write \"**/*.{js,css,scss,md}\"",
    "fix:eslint": "eslint --fix ./",
    "test:eslint": "eslint ./ --ext .js --ignore-pattern node_modules/",
    "test:unit": "nyc ./node_modules/.bin/mocha specs",
    "cover:report": "nyc report --reporter=lcov --reporter=text"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run fix:eslint",
      "pre-push": "npm run test:unit"
    }
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "express": "~4.16.1",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "sequelize": "^6.3.5",
    "sqlite3": "^5.0.1",
    "tedious": "^9.2.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^7.17.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.7",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1"
  }
}
